        <a id="vote-up-button-${comment.id}" command="voteUp"  href="#" class="make-tooltip"
           title="Yukarı oy ver">Beğen </a>
        <a id="vote-down-button-${comment.id}" command="voteDown" href="#" class="make-tooltip"
         title="Yukarı oy ver"><i class=" icon-chevron-down"></i></a>

        {{if crel.canComment }}
            <a id="comment-${comment.id}-reply" href="#" class="comment-reply"><i
                class="icon-reply"></i> Yanıtla</a>
        {{/if}}

        <a href="#" class="ajax-command"
                           command="share">Paylaş</a>

        {{if (crel.canReport || crel.canCancelReport) || crel.canEdit || crel.canDelete || crel.canConvertToAnswer }}

        <div class="dropdown" style="display:inline">
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                <i class=" icon-cog"></i> Daha fazla...</a>
            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                {{if crel.canReport || crel.canCancelReport }}
                    <li><a rel="nofollow" class="ajax-coxmmand {{if crel.reported }} on{{/if}}" href="#"
                           command="reportPost">


                        {{if !crel.reported }}123Flag{{else}}label.report.cancel{{/if}}
                        {{if crel.canViewReports }}(${comment.reportCount}){{/if}}
                    </a></li>
                {{/if}}
                {{if crel.canViewRevisions }}
                    <li class="item">
                        <a rel="nofollow" class="node-tools-revisions-link"
                           href="${revisionUrl}">See revisions</a>
                    </li>

                {{/if}}
                {{if crel.canEdit }}
                    <li><a id="comment-${comment.id}-edit" href="#"
                           class="comment-edit">Düzenle</a>
                    </li>
                {{/if}}
                {{if crel.canDelete }}
                    <li><a id="comment-${comment.id}-delete" href="#" class="comment-delete"
                           command="deleteComment">Sil </a>
                    </li>
                {{/if}}
                {{if crel.canConvertToAnswer }}
                    <li><a href="#" class="convert-to-answer"
                           command="convertToAnswer">Cevaba dönüştür</a>
                    </li>
                {{/if}}
            </ul>
        </div>

        {{/if}}

</div>
</div>
    <div id="comment-${comment.id}-left" class="comment-left">
        <div id="post-${comment.id}-score" class="comment-score"></div>
    </div>
    <div class="comment-text">{{html comment.bodyAsHTML}}</div>
</div>
</div>
</script>
