                "sidr",
                function(){
                    $(document.body).prepend("<div class='cover'></div>");
                    $(".cover").fadeIn().on("click", function(){
                        console.log("click");

                        $.sidr('close', 'sidr');
                        $(this).remove();
                    });
                }
            );
        });
    });


    $(".answer-body pre").addClass("prettyprint").addClass("linenums");
    $(".question-body pre").addClass("prettyprint").addClass("linenums");
    prettyPrint();

